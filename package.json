{
  "name": "ai-insight-generator",
  "version": "1.0.0",
  "description": "노트를 읽고 AI API를 사용하여 인사이트를 생성하는 Obsidian 플러그인",
  "main": "main.js",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --format=cjs --platform=browser --target=es2020 --outfile=main.js --minify --external:obsidian",
    "build:watch": "esbuild src/main.ts --bundle --format=cjs --platform=browser --target=es2020 --outfile=main.js --watch --external:obsidian"
  },
  "dependencies": {
    "obsidian": "latest",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "esbuild": "^0.18.0",
    "typescript": "^5.0.0"
  }
}
